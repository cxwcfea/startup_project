    <link rel="stylesheet" href="{{static '/vendor/toastr/toastr.css'}}">
    <link rel="stylesheet" href="{{static '/css/site.css'}}">
    -->
    <link rel="stylesheet" href="{{static '/css/niujinwang.min.d3b0a566.css'}}">
    {{#if bootstrappedUser}}
        <script> window.bootstrappedUserObject = {{{bootstrappedUser}}} </script>
    {{/if}}
</head>
<body ng-app="myApp" ng-controller="UserIndexController as indexVM">
